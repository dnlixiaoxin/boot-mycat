<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxx.boot.mapper.UsersMapper">

	<insert id="add" useGeneratedKeys="true" keyProperty="id">
	    insert into tb_user (name,update_time) values(#{name},now())
	</insert>
	
	<select id="find" resultType="com.lxx.boot.model.Users">
		select * from tb_user
	</select>
	

</mapper>